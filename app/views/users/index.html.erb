<%= render 'books/userinfo' , book: @book , user: @user %>

<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th>   </th>
    </tr>
  </thead>
<% @users.each do |user| %>
<tbody class="center">
  <tr>
  <td><%= image_tag user.get_profile_image(100,100) %></td>
  <td><%= user.name %></td>
  <td><%= link_to "show", user_path(user.id) %> </td>
  </tbody>
  <% end %>
</table>